{
  "name": "backend",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "dev:server": "dotenv -e .env.server -- nodemon --watch src -e ts --exec \"ts-node src/server.ts\"",
    "dev:worker": "dotenv -e .env.worker -- nodemon --watch src -e ts --exec \"ts-node src/worker.ts\"",
    "build": "prisma generate && tsc && echo 'TypeScript compilation completed'",
    "start": "node dist/server.js",
    "start:worker": "RUN_WORKER=true node ./dist/worker.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:studio": "dotenv -e .env -- prisma studio",
    "import:excel": "node scripts/importExcel.js",
    "import:multipleExcel": "node scripts/importMultipleExcel.js",
    "seed:managers": "node scripts/seedManagers.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "Backend API for Influencer CRM",
  "dependencies": {
    "@google-cloud/storage": "^7.17.3",
    "@prisma/client": "^6.19.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/ioredis": "^4.28.10",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^7.0.3",
    "@types/ws": "^8.18.1",
    "@types/xlsx": "^0.0.35",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.63.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "express-validator": "^7.3.0",
    "form-data": "^4.0.5",
    "googleapis": "^164.1.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "mailgun.js": "^12.1.1",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.10",
    "pg": "^8.16.3",
    "prisma": "^6.19.0",
    "socket.io": "^4.8.1",
    "ws": "^8.18.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "dotenv-cli": "^11.0.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
